# mount point	    fstype		device			device2			flags
# Symlinked partitions
/system_image		emmc	/dev/block/bootdevice/by-name/system		flags=backup=0;flashimg;display="System Image";wipeingui
/vendor_image		emmc	/dev/block/bootdevice/by-name/vendor		flags=backup=0;flashimg;display="Vendor Image";wipeingui
/product_image		emmc	/dev/block/bootdevice/by-name/product		flags=backup=0;flashimg;display="Product Image";wipeingui
/odm_image		emmc	/dev/block/bootdevice/by-name/odm		flags=backup=0;flashimg;display="Odm Image";wipeingui
/system_ext_image	emmc	/dev/block/bootdevice/by-name/system_ext	flags=backup=0;flashimg;display="System_EXT Image";wipeingui
/vendor_dlkm_image	emmc	/dev/block/bootdevice/by-name/vendor_dlkm	flags=backup=0;flashimg;display="Vendor_DLKM Image";wipeingui
/system_dlkm_image	emmc	/dev/block/bootdevice/by-name/system_dlkm	flags=backup=0;flashimg;display="System_DLKM Image";wipeingui

# Physical partitions
/boot               emmc      /dev/block/bootdevice/by-name/boot               flags=display=boot
/recovery           emmc      /dev/block/bootdevice/by-name/recovery           flags=display=recovery
/metadata           ext4      /dev/block/by-name/metadata                      flags=display=metadata
/data               f2fs      /dev/block/bootdevice/by-name/userdata           flags=display=data
/cache              ext4      /dev/block/bootdevice/by-name/cache              flags=display=cache
/efs                ext4      /dev/block/bootdevice/by-name/sec_efs            flags=display=efs
/sec_efs            ext4      /dev/block/bootdevice/by-name/sec_efs            flags=display=sec_efs
/carrier            ext4      /dev/block/bootdevice/by-name/carrier            flags=display=carrier
/modem              emmc      /dev/block/bootdevice/by-name/apnhlos            flags=display=modem
/mdm                emmc      /dev/block/bootdevice/by-name/modem              flags=display=mdm
/dsp                emmc      /dev/block/bootdevice/by-name/dsp                flags=display=dsp
/misc               emmc      /dev/block/bootdevice/by-name/misc               flags=display=misc
/keydata            ext4      /dev/block/bootdevice/by-name/keydata            flags=display=keydata
/keyrefuge          f2fs      /dev/block/bootdevice/by-name/keyrefuge          flags=display=keyrefuge
/dtbo               emmc      /dev/block/bootdevice/by-name/dtbo               flags=display=dtbo
/prism              ext4      /dev/block/bootdevice/by-name/prism              flags=display=prism
/optics             ext4      /dev/block/bootdevice/by-name/optics             flags=display=optics
/vbmeta_system      emmc      /dev/block/bootdevice/by-name/vbmeta_system      flags=display=vbmeta_system
/vendor_boot        emmc      /dev/block/bootdevice/by-name/vendor_boot        flags=display=vendor_boot
/vbmeta_samsung     emmc      /dev/block/bootdevice/by-name/vbmeta_samsung     flags=display=vbmeta_samsung
/imagefv            emmc      /dev/block/bootdevice/by-name/imagefv            flags=display=imagefv
/sdcard             sdfat     /dev/block/mmcblk0p1                             flags=display=sdcard
/init_boot          emmc      /dev/block/bootdevice/by-name/init_boot          flags=display=init_boot
/vm-bootsys         ext4      /dev/block/bootdevice/by-name/vm-bootsys         flags=display=vm-bootsys
/qweslicstore       emmc      /dev/block/bootdevice/by-name/qweslicstore       flags=display=qweslicstore

# Removable storage
/usb-otg	vfat		/dev/block/sde1	/dev/block/sde			flags=display="USB-OTG";storage;wipeingui;removable